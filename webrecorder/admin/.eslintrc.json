{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y"
        /*"import"*/
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-closing-bracket-location": 0,
        "quotes": [1, "single"],
        "jsx-quotes": ["warn", "prefer-single"],
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "class-methods-use-this": 0,
        "no-console": 0,
        "keyword-spacing": 0
    },
    "globals" : {
        "window": true,
        "document": true,
        "fetch": true,
        "FormData": true
    }
}